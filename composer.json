{
  "name": "inpsyde/google-tag-manager",
  "description": "Adding GTM to your site.",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "ChriCo",
      "email": "c.brueckner@inpsyde.com",
      "homepage": "http://www.chrico.info"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:Chrico/chrico-wp-fields.git"
    }
  ],
  "support": {
    "src": "bitbucket.org/inpsyde/inpsyde-google-tag-manager/"
  },
  "require": {
    "php": ">=7",
    "pimple/pimple": "~3.0",
    "brain/nonces": "1.0.*",
    "chrico/wp-fields": "~0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "6.3.*",
    "brain/monkey": "^2",
    "wp-cli/wp-cli": "~1.3",
    "paulgibbs/behat-wordpress-extension": "^0.9.0",
    "behat/mink-goutte-driver": "^1.2",
    "behat/mink-selenium2-driver": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Inpsyde\\GoogleTagManager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Inpsyde\\GoogleTagManager\\Tests\\": "tests/phpunit/"
    }
  },
  "suggest": {
    "inpsyde/wonolog": "You may want to install Wonolog to enable logging for this package."
  }
}
